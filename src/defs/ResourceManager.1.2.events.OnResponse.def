message <span class="message">Response</span>
{
  required <span class="number">uint32</span> requestID    = 1; 
  required <span class="number">uint32</span> resourceID   = 2; 
  <span class="comment">/** 
   * Milliseconds since Unix epoch.
   */</span>
  required <span class="number">double</span> time         = 3; 
  <span class="comment">/** 
   * HTTP response code, such as 200, 404, etc.
   */</span>
  optional <span class="number">uint32</span> responseCode = 4; 
}
