  <span class="comment">/**
    * "line", "function", "event"
    */</span>
  required <span class="string">string</span> type = 2;
  <span class="comment">/**
    * Present when `type` is `"line"`
    */</span>
  optional <span class="number">uint32</span> scriptID = 3;
  <span class="comment">/**
    * Present when `type` is `"line"`
    */</span>
  optional <span class="number">uint32</span> lineNumber = 4;
  <span class="comment">/**
