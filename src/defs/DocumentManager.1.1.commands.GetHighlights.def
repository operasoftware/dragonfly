message <span class="message">GetHighlightsArg</span>
{
  <span class="comment">/** 
   * Get highlights in this window.
   */</span>
  required <span class="number">uint32</span> windowID = 1; 
  <span class="comment">/** 
   * If set, search only in the specified frame. The frame must be
   * part of the document tree of the previously specified window.
   */</span>
  optional <span class="number">uint32</span> frameID  = 2; 
}
