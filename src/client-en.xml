    "fi": "fi",
    "fr": "fr",
    "fr-CA": "fr-CA",
    "fy": "fy",
    "hi": "hi",
    "hr": "hr",
    "hu": "hu",
    "id": "id",
    "it": "it",
    "ja": "ja",
    "ka": "ka",
    "ko": "ko",
    "lt": "lt",
    "mk": "mk",
    "nb": "nb",
    "nl": "nl",
